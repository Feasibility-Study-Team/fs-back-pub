// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model inventor{
  id_inventor Int @id @default(autoincrement())
  username String
  email String
  nama_lengkap String
  id_institusi Int
  password String
  nomor String
}